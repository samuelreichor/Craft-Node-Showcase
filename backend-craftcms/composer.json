{
  "require": {
    "craftcms/cms": "4.7.0",
    "craftcms/redactor": "3.0.4",
    "doublesecretagency/craft-cpcss": "2.6.0",
    "fredmansky/craft-custom-login": "1.0",
    "mmikkel/cp-field-inspect": "1.4.4",
    "nystudio107/craft-emptycoalesce": "4.0.0",
    "nystudio107/craft-seomatic": "4.0.38",
    "nystudio107/craft-templatecomments": "4.0.0",
    "nystudio107/craft-twigprofiler": "4.0.0-beta.1",
    "putyourlightson/craft-blitz-recommendations": "2.1.3",
    "putyourlightson/craft-elements-panel": "2.0.0",
    "sebastianlenz/linkfield": "2.1.5",
    "spacecatninja/imager-x": "4.2.4",
    "studioespresso/craft-dumper": "3.0.1",
    "verbb/formie": "2.1.3",
    "verbb/image-resizer": "3.0.10",
    "verbb/navigation": "2.0.24",
    "verbb/smith": "2.0.0",
    "verbb/super-table": "3.0.12",
    "vlucas/phpdotenv": "5.4.0"
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "1.10.1",
    "roave/security-advisories": "dev-latest",
    "yiisoft/yii2-shell": "2.0.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "cweagans/composer-patches": true,
      "yiisoft/yii2-composer": true,
      "php-http/discovery": true
    }
  },
  "scripts": {
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "create-folders": [
      "mkdir -p ./storage",
      "mkdir -p ./config/project",
      "mkdir -p ./web/cpresources",
      "mkdir -p ./web/assets"
    ],
    "pre-craft-update": [
      "@create-folders"
    ],
    "post-craft-update": [
      "@php craft install/check && php craft clear-caches/compiled-templates || exit 0",
      "@php craft install/check && php craft clear-caches/data || exit 0",
      "@php craft install/check && php craft clear-caches/temp-files || exit 0",
      "@php craft install/check && php craft migrate/all --no-content || exit 0",
      "@php craft install/check && php craft project-config/apply --force || exit 0",
      "@php craft install/check && php craft migrate --track=content || exit 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  },
  "repositories": [
    {
      "url": "git@github.com:fredmansky/craft-custom-login.git",
      "type": "git"
    },
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
